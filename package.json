{
  "private": false,
  "name": "@r00t80y/monorepo",
  "description": "Array shuffle",
  "author": "nnu17ip1y <nnu17ip1y@yahoo.com> (https://github.com/R00T80Y)",
  "version": "0.0.7",
  "license": "MIT",
  "keywords": [
    "array",
    "shuffle"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "dev": "pnpm --filter='./packages/shuffle' dev",
    "test": "pnpm --filter='./packages/shuffle' test",
    "build": "pnpm --filter='./packages/shuffle' build",
    "lint": "pnpm --filter='./packages/shuffle' lint",
    "ci": "pnpm --filter='./packages/shuffle' ci",
    "release": "pnpm ci && changeset publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "@repo/prettier-config": "workspace:*"
  },
  "packageManager": "pnpm@7.15.0"
}
